# Cloudflare R2 Configuration
# Copy this file to .env and fill in your actual values

# Your Cloudflare Account ID (found in Cloudflare Dashboard)
CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# R2 API Credentials (create in Cloudflare Dashboard > R2 > Manage R2 API Tokens)
R2_ACCESS_KEY_ID=your_access_key_id_here
R2_SECRET_ACCESS_KEY=your_secret_access_key_here

# R2 Bucket Name (create in Cloudflare Dashboard > R2)
R2_BUCKET_NAME=football-logos

# Public URL for R2 bucket (set up custom domain or use r2.dev subdomain)
# Example: https://logos.yourdomain.com or https://pub-xxxxx.r2.dev
R2_PUBLIC_URL=https://your-r2-public-url-here
